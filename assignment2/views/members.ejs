<%- include("templates/header") %>

    <% if (!req.session.authenticated) { %>
        <% res.redirect('/login'); %>
    <% } %>

    <h1>Members only</h1>
    <h2>Hello, <%= username %></h2>
    <div class="container mt-5">
        <div class="row">
          <div class="col-sm">
            <img src="<%= pic1 %>">
          </div>
          <div class="col-sm">
            <img src="<%= pic2 %>">
          </div>
          <div class="col-sm">
            <img src="<%= pic3 %>">
          </div>
        </div>
    </div>
    <button class="btn btn-primary rounded-pill px-3" type="button" onclick="window.location.href='/logout'">Logout</button>

<%- include("templates/footer") %>
